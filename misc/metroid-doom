usage() {
printf "usage: `basename $0` \n\
   [-m --mini mini] \n\
   [-t --text text] \n\
   [-n --normal normal] \n\
   [-h --help help]\n"
}

text() {
	printf "\n  .___.\n /  @  \ \n \ @ @ /\n  {'^'}\n\n"
}

